# ignore everything in the root except the "wp-content" directory.
!wp-content/

# ignore everything in the "wp-content" directory, except:
# "plugins" directory
# "themes" directory
!/wp-content/upgrade/
!/wp-content/updraft/
!/wp-content/index.php

# config files
/wp-config.php
/whitelist.php


# ignore log files and databases
*.log
*.sql
*.sqlite
*.mmdb
error_log

# ignore compiled files
*.com
*.class
*.dll
*.exe
*.o
*.so
*.ftpquota

# ignore packaged files
*.7z
*.dmg
*.gz
*.iso
*.jar
*.rar
*.tar
*.zip

# ignore Editor files
*.sublime-project
*.sublime-workspace
*.idea

# ignore OS generated files
ehthumbs.db
Thumbs.db
.directory

# Cache files
*.cache

# Other
/cgi-bin/
/cgi-bin/*
/test.php
/mega-brokers.co/
/services/mixpanel-php-master/
/wp-content/themes/1800option/dev-only/
/robots.txt
